{
  "name": "react-plotly.js",
  "version": "2.6.0",
  "engines": {
    "node": ">= 18"
  },
  "license": "MIT",
  "description": "A plotly.js react component from Plotly",
  "author": "Plotly, Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/plotly/react-plotly.js.git"
  },
  "bugs": {
    "url": "https://github.com/plotly/react-plotly.js/issues"
  },
  "main": "dist/cjs/react-plotly.cjs",
  "module": "dist/react-plotly.legacy-esm.js",
  "types": "dist/cjs/react-plotly.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/react-plotly.d.mts",
      "import": "./dist/react-plotly.mjs",
      "default": "./dist/cjs/react-plotly.cjs"
    },
    "./factory": {
      "types": "./dist/factory.d.mts",
      "import": "./dist/factory.mjs",
      "default": "./dist/cjs/factory.cjs"
    }
  },
  "scripts": {
    "lint": "prettier --trailing-comma es5 --write \"src/**/*.js\" && eslint src",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "keywords": [
    "graphing",
    "plotting",
    "data",
    "visualization",
    "plotly",
    "react"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.0.2",
    "@vitest/ui": "^1.0.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "event-emitter": "^0.3.5",
    "jsdom": "^23.0.1",
    "onetime": "^7.0.0",
    "prettier": "^3.1.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.0.2",
    "vitest-canvas-mock": "^0.3.3"
  },
  "peerDependencies": {
    "plotly.js-dist-min": "^2.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
